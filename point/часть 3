#include <stdio.h>

void getUserData(int *t1,int *t2,int *t3,int *t4,int *t5);
void printData(int *t1, int *t2, int *t3, int *t4, int* t5);

int main()
{
	int t1;
	int t2;
	int t3;
	int t4;
	int t5;
	getUserData(&t1, &t2, &t3, &t4, &t5);
	printData(&t1, &t2, &t3, &t4, &t5);
}

void getUserData(int *t1, int *t2, int *t3, int *t4, int *t5)
{

		printf("Enter t1: ");
		scanf("%i",t1);
		printf("Enter t2: ");
		scanf("%i", t2);
		printf("Enter t3: ");
		scanf("%i", t3);
		printf("Enter t4: ");
		scanf("%i", t4);
		printf("Enter t5: ");
		scanf("%i", t5);
}

void printData(int* t1, int *t2, int* t3, int *t4, int *t5)
{
	printf("%i\n", *t1);
	printf("%i\n", *t2);
	printf("%i\n", *t3);
	printf("%i\n", *t4);
	printf("%i\n", *t5);
}
