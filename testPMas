#include <stdio.h>
#include <limits.h>

void getArray(int matrix[][7], int arr1[5],int arr2[7]);
void printArray(int matrix[][5]);

int main()
{
	int arr1[5];
	int arr2[7];
	int matrix[2][7];
	getArray(matrix, arr1, arr2[7]);
}
void getArray(int matrix[][7], int arr1[5],int arr2[7])
{
	for (int i = 0; i < 7;i++)
	{
		if (i < 5)
		{
			matrix[0][i] = arr1[i];
		}
	}
}
